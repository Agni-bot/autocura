# Configuração Concreta: meuapp.conf
# Este é um arquivo de configuração real para MeuApp.

# Configurações Gerais
app_name = MeuAppServicoPrincipal
app_version = 1.0.2
log_level = DEBUG # Opções: DEBUG, INFO, WARNING, ERROR, CRITICAL
log_file = /var/log/meuapp/meuapp.log

# Configurações de Rede
listen_address = 0.0.0.0
listen_port = 8080

# Configurações de Banco de Dados (Exemplo PostgreSQL)
db_host = localhost
db_port = 5432
db_user = meuapp_prod_user
db_password = "P@$$wOrd_S3gur0_123!"
db_name = meuapp_production_db
